generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id       String    @id @default(uuid())
  username String
  password String
  Session  Session[]
}

model Session {
  id     String @id @default(uuid())
  userId String
  user   Users   @relation(references: [id], fields: [userId])
  expiresAt DateTime
}
